/* 轮播图 */

.swiper-container {
  padding: 10px 15px;
}

.swiper-border {
  border-radius: 10px;
}

.swiper-height {
  height: 280rpx;
}

.imageclass {
  height: 280rpx;
  width: 100%;
}

/* 表格 */

.display-container {
  display: flex;
  border-bottom: 1px solid #f3f3f3;
}

.display-container:nth-last-child(1) {
  border-bottom: 0px solid #f3f3f3;
}

.box-shawss {
  box-shadow: -2px -1px 12px 1px rgb(246, 246, 246), 2px 1px 12px 1px rgb(246, 246, 246);
}
/* 表格内容 */
.table-left {
  flex: 1;
  padding: 20px 15px;
  height: 100rpx;
  display: flex;
  align-items: center;
  border-right: 1px solid #f3f3f3;
}
.table-right {
  flex: 1;
  padding: 20px 15px;
  display: flex;
  align-items: center;
}
.flex-value-one {
  flex: 1;
}
.container-text-bold {
  font-weight: bold;
  display: block;
}
.container-text-tint {
  color: rgb(189, 189, 189);
  font-size: 20rpx;
  display: block;

}

/* 图标 */
.icon-face {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10px;
  background-repeat: no-repeat;
  background-size: 50rpx 50rpx;
}
